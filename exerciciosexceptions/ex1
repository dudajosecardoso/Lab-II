def equilatero():
    lado1 = float(input("Digite o lado 1 do triângulo: "))
    lado2 = float(input("Digite lado 2 do triângulo: "))
    lado3 = float(input("Digite o lado 3 do trinagulo: "))
    
    maior_lado = max(lado1, lado2, lado3)
   
    menor1 = min(lado1, lado2, lado3)
    menor2 = lado1 + lado2 + lado3 - maior_lado - menor1
   
   
    sum = menor1 + menor2
    
    print(menor1)
    print(menor2)
    print(maior_lado)
    
    if sum < maior_lado:
        print("O triângulo é equilatero")
    else:
        print("O triângulo não é equilatero")
        
equilatero()
