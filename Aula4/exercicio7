def count_elements(lst):
    result = {}
    
    for n in lst:
        if n in result:
            result[n] += 1
        else:
            result[n] = 1
    
    return result

def main():
    lst = [ ]
    for x in range(5):
        number = int(input("Digite um nÃºmero inteiro: ")
        lst.append(number)
    result = count_elements(lst)
    print(result)
main()
